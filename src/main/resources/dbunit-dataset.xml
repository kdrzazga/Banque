<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE dataset [
	<!ELEMENT dataset (FUNDS |PARTIES |TRADES)*>

	<!ELEMENT FUNDS EMPTY>
	<!ATTLIST FUNDS
	        ID CDATA #REQUIRED
	        NAME CDATA #REQUIRED
	        SHORTNAME CDATA #REQUIRED

