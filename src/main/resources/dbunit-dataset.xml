<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE dataset [
        <!ELEMENT dataset (PARTIES | FUNDS | TRADES)*>
        <!ELEMENT PARTIES EMPTY>
        <!ATTLIST PARTIES
                id CDATA #REQUIRED
                NAME CDATA #REQUIRED
                shortName CDATA #REQUIRED>
        <!ELEMENT FUNDS EMPTY>
        <!ATTLIST FUNDS
                id CDATA #REQUIRED
                NAME CDATA #REQUIRED
                shortName CDATA #REQUIRED
                units CDATA #REQUIRED
                PARTY_ID CDATA #IMPLIED
                TRADE_ID CDATA #IMPLIED>
        <!ELEMENT TRADES EMPTY>
        <!ATTLIST TRADES
                id CDATA #REQUIRED
                quantity CDATA #REQUIRED>
        ]>

<dataset>
    <PARTIES id="1" NAME="Employees Provident Fund" shortName="EPF"/>
    <PARTIES id="2" NAME="Ontario Teachers' Pension Plan" shortName="OTPP"/>

    <TRADES id="1" quantity="100"/>

    <FUNDS id="1" NAME="IBM" shortName="IBM" PARTY_ID="1" units="120.5" TRADE_ID="1"/>
    <FUNDS id="2" NAME="Microsoft" shortName="MSFT" units="20.75" />
    <FUNDS id="3" NAME="Apple" shortName="APPL" PARTY_ID="1" units="1525" />
</dataset>
