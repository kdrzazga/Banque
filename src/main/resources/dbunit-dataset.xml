<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE dataset [
        <!ELEMENT dataset (counterparties | funds | teams)*>
        <!ELEMENT counterparties EMPTY>
        <!ATTLIST counterparties
                id CDATA #REQUIRED
                name CDATA #REQUIRED>
        <!ELEMENT funds EMPTY>
        <!ATTLIST funds
                id CDATA #REQUIRED
                name CDATA #REQUIRED
                shortName CDATA #REQUIRED
                counterparty_id CDATA #IMPLIED
                team_id CDATA #IMPLIED>
        <!ELEMENT teams EMPTY>
        <!ATTLIST teams
                id CDATA #REQUIRED
                name CDATA #REQUIRED>
        ]>

<dataset>
    <counterparties id="1" name="Employees Provident Fund"/>
    <counterparties id="2" name="Ontario Teachers' Pension Plan"/>

    <teams id="1" name="Super team"/>

    <funds id="1" name="IBM" shortName="IBM" counterparty_id="1" team_id="1"/>
    <funds id="2" name="MSFT" shortName="Microsoft"/>
    <funds id="3" name="APPL" shortName="Apple" counterparty_id="1"/>
</dataset>
